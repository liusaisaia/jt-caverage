{
  "name": "@jt-coverage/vue3",
  "version": "0.0.23",
  "description": "Vue3 adapter for jt-coverage. Supports webpack-chain (Vue CLI 5) and a minimal Vite plugin.",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./index": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./lib/CoverageButton.vue": {
      "import": "./lib/CoverageButton.vue",
      "require": "./lib/CoverageButton.vue",
      "types": "./lib/CoverageButton.vue"
    },
    "./lib/*": {
      "import": "./lib/*.vue",
      "require": "./lib/*.vue",
      "types": "./lib/*.vue"
    },
    "./package.json": "./package.json"
  },
  "type": "commonjs",
  "license": "MIT",
  "keywords": [
    "coverage",
    "istanbul",
    "vue3",
    "vite",
    "webpack-chain"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "lib/**/*"
  ],
  "peerDependencies": {
    "dayjs": ">=1.7.0",
    "vite": ">=2.0.0",
    "vite-plugin-istanbul": "^3.0.0 || ^4.0.0 || ^7.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vite-plugin-istanbul": {
      "optional": true
    }
  },
  "dependencies": {
    "@jt-coverage/core": "0.0.1"
  },
  "publishConfig": {
    "registry": "https://maven.jtexpress.com.cn/nexus3/repository/npm-hosted-2/",
    "access": "public"
  },
  "scripts": {
    "build": "echo @jt-coverage/vue3 build OK"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
