{
  "name": "jt-coverage",
  "version": "0.0.28Beta71",
  "description": "A wrapper for coverage-source-map-trace-plugin, babel-plugin-istanbul and nyc.",
  "main": "dist/jt-coverage.cjs.js",
  "type": "commonjs",
  "workspaces": ["packages/core", "packages/vue2"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pub:c": "npm publish --registry=https://maven.jtexpress.com.cn/nexus3/repository/npm-hosted-2/",
    "build": "vite build",
    "pub:core": "npm publish --workspace @jt-coverage/core --registry=https://maven.jtexpress.com.cn/nexus3/repository/npm-hosted-2/ --access=public",
    "pub:vue2": "npm publish --workspace @jt-coverage/vue2 --registry=https://maven.jtexpress.com.cn/nexus3/repository/npm-hosted-2/ --access=public",
    "pub:vue3": "npm publish --workspace @jt-coverage/vue3 --registry=https://maven.jtexpress.com.cn/nexus3/repository/npm-hosted-2/ --access=public"
  },
  "keywords": [
    "coverage",
    "source-map",
    "babel",
    "nyc",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "dayjs": ">=1.7.0",
    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "vite": "^2.9.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "1.1.2",
    "babel-plugin-istanbul": "^6.0.0",
    "coverage-source-map-trace-plugin": "^1.0.0"
  },
  "files": ["dist", "lib"],
  "nyc": {
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "**/*.spec.js",
      "node_modules/"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  }
}
